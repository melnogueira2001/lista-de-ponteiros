float aloha[10], coisas[10][5], *pf, value = 2.2;
int i=3;

//Verificando a validade dos comandos:

aloha[2] = value; é válido

scanf("%f", &aloha); é válido

aloha = value; é inválido

printf("%f", aloha); é inválido

coisas[4][4] = aloha[3]; é válido

coisas[5] = aloha; é inválido

pf = value; é inválido

pf = aloha; é válido
